{
    "runtime": {
        "version": "LuaJIT",
        "path": [
            "lua/?.lua",
            "lua/?/init.lua"
        ]
    },

    "diagnostics": {
        "globals": ["vim"],
        "disable": [
            "unused-local",
            "undefined-field"
        ]
    },

    "workspace.checkThirdParty": false,

    "workspace.library": [
        "$VIMRUNTIME"
    ]
}
